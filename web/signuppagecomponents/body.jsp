<div id="mainSplitContainer">

		<div class="sub" id="subicon">
		</div>

		<div></div>

		<div class="sub">
			<h3 class="center-align">User Signup</h3>
			<div class="input-field">
				<input id="emailid_signup" type="email">
				<label for="emailid" class="input_label">Email</label>
			  </div>
			<div class="input-field">
				<input id="uname_signup" type="text">
				<label for="uname" class="input_label">Username</label>
			  </div>
			  <div class="input-field">
				<input id="age_signup" type="number" min="1" max="200" step="1">
				<label for="age" class="input_label">Age</label>
			  </div>
				<div class="switch">
					<label>
						<strong style="font-size:1rem;" class="grey-text text-darken-4">Customer</strong>
							<input type="checkbox" id="type_signup">
							<span class="lever"></span>
						<strong style="font-size:1rem;" class="grey-text text-darken-4">Theater Owner</strong>
					</label>
				</div>
			  <div class="input-field">
				<input id="pwd_signup" type="password">
				<label for="pwd" class="input_label">Password</label>
			  </div>
			  

			  <button class="btn-large z-depth-2" id="signup_button">Sign Up</button>
			  <br>
			  <a style="postion:relative;" href="/MovieTicketsTiles/">Already have an account ? Log In here</a>
		</div>

	</div>